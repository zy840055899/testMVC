<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.test.dao.TestDao">

    <select id="testSql" resultType="com.test.entity.Student">
        SELECT * FROM student
    </select>

    <select id="getStuById" parameterType="int" resultType="com.test.entity.Student">
        SELECT * FROM student WHERE id=#{id}
    </select>

    <select id="getNeedStaticStus" resultType="com.test.entity.Student">
        SELECT * FROM student
        WHERE static_time IS NULL
        OR student.update_time>student.static_time
    </select>

</mapper>


